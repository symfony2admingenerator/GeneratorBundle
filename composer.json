{
    "name": "symfony2admingenerator/generator-bundle",
    "minimum-stability": "stable",
    "description": "Admingenerator for Symfony2 based on YAML configuration and Twig templating",
    "keywords": [
        "admin",
        "generator",
        "yaml"
    ],
    "type": "symfony-bundle",
    "homepage": "http://symfony2admingenerator.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Cedric LOMBARDOT",
            "email": "cedric.lombardot@gmail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "http://github.com/symfony2admingenerator/GeneratorBundle/contributors"
        }
    ],
    "require": {
        "symfony/symfony": ">=2.8,<4.0",
        "symfony/assetic-bundle": "^2.7,>=2.7.1",
        "doctrine/common": "~2.2",
        "sensio/generator-bundle": "~3.0",
        "symfony2admingenerator/twig-generator": "~1.2,>=1.2.2",
        "pagerfanta/pagerfanta": "dev-adaptater-propel2",
        "white-october/pagerfanta-bundle": "dev-adaptater-propel2",
        "twig/twig": "~1.12",
        "twig/extensions": "~1.0"
    },
    "require-dev": {
        "doctrine/orm": ">=2.2.3",
        "doctrine/dbal": ">=2.2.3",
        "phpunit/phpunit": "~4.3"
    },
    "suggest": {
        "symfony2admingenerator/form-extensions-bundle": "Symfony2 form extensions",
        "symfony2admingenerator/user-bundle": "FOSUserBundle integration for Admingenerator",
        "liuggio/excelbundle": "(version ^2.0) Allows the usage of the Excel export.",
        "jms/security-extra-bundle": "Take advantages of JMS Security Extra Bundle in your generated templates.",
        "cnerta/breadcrumb-bundle": "(version ^2.0) Allows the usage of breadcrumbs.",
        "knplabs/knp-menu-bundle": "(version >1.0,<2.2) Allows automatic rendering of the menu structure."
    },
    "autoload": {
        "psr-4": {
            "Admingenerator\\GeneratorBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "repositories": [
       {
         "type": "git",
         "url": "https://github.com/donkeycode/Pagerfanta.git"
       },
       {
         "type": "git",
         "url": "https://github.com/donkeycode/WhiteOctoberPagerfantaBundle.git"
       }
   ]
}
