{% use '@AdmingeneratorGenerator/templates/CommonAdmin/ListTemplate/row.php.twig' %}

{% block list_row %}
    {{ echo_block('list_row') }}
        <tr role="row"
            data-tt-id="node-{{ echo_twig(builder.ModelClass ~ "." ~ builder.getFieldGuesser().getModelPrimaryKeyName(model)) }}"
            {{ echo_if(builder.ModelClass ~ "." ~ builder.treeConfiguration.parent) }}
                data-tt-parent-id="node-{{ echo_twig(builder.ModelClass ~ "." ~ builder.treeConfiguration.parent ~ "." ~ builder.getFieldGuesser().getModelPrimaryKeyName(model)) }}"
            {{ echo_endif() }}
            class="list-results-fields-row">
            {{- block('list_row_content') -}}
        </tr>
    {{ echo_endblock() }}
{% endblock list_row %}