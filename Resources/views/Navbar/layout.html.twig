<nav class="navbar navbar-static-top" role="navigation">
    <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
    </a>

    <div class="navbar-right">
        <ul class="nav navbar-nav">
            {% block navbar_list_container %}
                {% block navbar_profile %}
                <li class="dropdown user user-menu">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <i class="fa fa-user"></i>
                        <span>{{ app.user ? app.user : 'profile.unauthenticated'|trans({}, 'Admingenerator') }} <i class="caret"></i></span>
                    </a>
                    <ul class="dropdown-menu">
                        {% if app.user %}
                            {% block navbar_profile_authenticated %}
                                {% include "AdmingeneratorGeneratorBundle:Navbar:authenticated.html.twig" %}
                            {% endblock navbar_profile_authenticated %}
                        {% else %}
                            {% block navbar_profile_not_authenticated %}
                                {% include "AdmingeneratorGeneratorBundle:Navbar:not_authenticated.html.twig" %}
                            {% endblock navbar_profile_not_authenticated %}
                        {% endif %}
                    </ul>
                </li>
                {% endblock navbar_profile %}
            {% endblock navbar_list_container %}
        </ul>
    </div>
</nav>
